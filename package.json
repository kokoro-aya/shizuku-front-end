{
  "private": true,
  "version": "2.0.0",
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/pro-layout": "^5.0.12",
    "@monaco-editor/react": "^4.4.2",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.3.1",
    "lint-staged": "^10.0.7",
    "monaco-editor": "^0.33.0",
    "prettier": "^1.19.1",
    "prismjs": "^1.22.0",
    "react": "^17.0.0",
    "react-contenteditable": "^3.3.5",
    "react-dom": "^17.0.0",
    "react-scroll": "^1.8.1",
    "umi": "^3.3.1",
    "yorkie": "^2.0.0"
  },
  "devDependencies": {
    "typescript": "^4.1.0"
  }
}
